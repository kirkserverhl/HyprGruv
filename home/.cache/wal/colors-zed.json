{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#1e1d1c90",
                "editor.background": "#1e1d1c90",
                "editor.foreground": "#c6c6c6",
                "text": "#c6c6c6",
                "text.muted": "#c6c6c670",
                "text.ignored": "#c6c6c640",
                "text.placeholder": "#c6c6c650",
                "ignored": "#c6c6c630",
                "element.hover": "#c6c6c630",
                "ghost_element.hover": "#928B7830",
                "ghost_element.selected": "#928B7830",
                "ghost_element.active": "#928B7860",
                "border": "#1e1d1c",
                "editor.highlighted_line.background": "#928B7810",
                "editor.active_line.background": "#928B7810",
                "panel.background": "#1e1d1c90",
                "title_bar.background": "#1e1d1c90",
                "title_bar.inactive_background": "#1e1d1c90",
                "status_bar.background": "#1e1d1c90",
                "drop_target.background": "#1e1d1c90",
                "elevated_surface.background": "#1e1d1c",
                "toolbar.background": "#1e1d1c90",
                "tab_bar.background": "#1e1d1c90",
                "tab.inactive_background": "#1e1d1c90",
                "tab.active_background": "#928B7830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#1e1d1c",
                "scrollbar.thumb.background": "#928B78",
                "editor.gutter.background": "#1e1d1c90",
                "terminal.background": "#1e1d1c10",
                "terminal.foreground": "#c6c6c6",
                "terminal.dim_foreground": "#c6c6c6",
                "terminal.bright_foreground": "#c6c6c6",
                "terminal.ansi.black": "#1e1d1c",
                "terminal.ansi.red": "#928B78",
                "terminal.ansi.green": "#A0967F",
                "terminal.ansi.yellow": "#9F9B86",
                "terminal.ansi.blue": "#B3AC91",
                "terminal.ansi.magenta": "#C4BB9D",
                "terminal.ansi.cyan": "#BAC0A6",
                "terminal.ansi.white": "#c6c6c6",
                "terminal.ansi.bright_black": "#767660",
                "terminal.ansi.bright_red": "#928B78",
                "terminal.ansi.bright_green": "#A0967F",
                "terminal.ansi.bright_yellow": "#9F9B86",
                "terminal.ansi.bright_blue": "#B3AC91",
                "terminal.ansi.bright_magenta": "#C4BB9D",
                "terminal.ansi.bright_cyan": "#BAC0A6",
                "terminal.ansi.bright_white": "#c6c6c6",
                "modified": "#928B78",
                "syntax": {
                    "attribute": {
                        "color": "#928B78"
                    },
                    "boolean": {
                      "color": "#A0967F"
                    },
                    "comment": {
                      "color": "#767660"
                    },
                    "comment.doc": {
                      "color": "#767660"
                    },
                    "constant": {
                      "color": "#A0967F"
                    },
                    "function": {
                      "color": "#BAC0A6"
                    },
                    "keyword": {
                      "color": "#B3AC91"
                    },
                    "number": {
                      "color": "#C4BB9D"
                    },
                    "operator": {
                      "color": "#B3AC91"
                    },
                    "string": {
                      "color": "#928B78"
                    },
                    "variable": {
                      "color": "#A0967F"
                    }
                },
                "players": [
                  {
                    "cursor": "#928B78",
                    "background": "#1e1d1c",
                    "selection": "#928B7830"
                  }
                ]
            }
        }
    ]
}
